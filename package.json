{
  "name": "rrule",
  "version": "2.8.0",
  "description": "JavaScript library for working with recurrence rules for calendar dates.",
  "homepage": "http://jakubroztocil.github.io/rrule/",
  "license": "BSD-3-Clause",
  "keywords": [
    "dates",
    "recurrences",
    "calendar",
    "icalendar",
    "rfc"
  ],
  "author": "Jakub Roztocil, Lars SchÃ¶ning, and David Golightly",
  "types": "./dist/commonjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "type": "module",
  "tshy": {
    "project": "./tsconfig.build.json",
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jakubroztocil/rrule.git"
  },
  "scripts": {
    "prepare": "tshy",
    "prebuild": "yarn clean",
    "prepublish": "yarn build",
    "build": "yarn lint && yarn format-check && tshy",
    "clean": "rm -rf dist/",
    "lint": "yarn eslint . --config .eslintrc.cjs",
    "format": "yarn prettier --write .",
    "format-check": "yarn prettier --check .",
    "run-ts": "TS_NODE_PROJECT=tsconfig.json node --loader ts-node/esm",
    "test": "jest **/*.test.ts",
    "test-ci": "yarn run-ts ./node_modules/.bin/nyc jest **/*.test.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "yarn format"
    ]
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/jest": "^29.5.8",
    "@types/mockdate": "^3.0.0",
    "@types/node": "^22.15.26",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "coverage": "^0.4.1",
    "eslint": "8",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.1",
    "mockdate": "^3.0.5",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "source-map-loader": "^3.0.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.1",
    "tshy": "^3.0.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=20.11.0",
    "yarn": ">=3"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js"
}
